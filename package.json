{
  "name": "action-local-cache",
  "version": "0.1.0",
  "private": true,
  "description": "A Github Action to save and restore files across job runs directly in the runner's file system",
  "main": "dist/main/index.js",
  "scripts": {
    "all": "yarn format && yarn lint && yarn type-check && yarn pack && yarn test",
    "format-check": "prettier --check **/*.ts",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build src/main.ts --out dist/main && ncc build src/post.ts --out dist/post",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redact-dev/action-local-cache.git"
  },
  "keywords": [
    "actions",
    "node",
    "self-hosted",
    "runner"
  ],
  "author": "johnpyp",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.5.0",
    "@actions/io": "1.1.1",
    "loglevel": "1.7.1"
  },
  "devDependencies": {
    "@stefanmaric/eslint-config-munchies-base": "0.3.0",
    "@stefanmaric/eslint-config-munchies-gourmet": "0.3.0",
    "@stefanmaric/eslint-config-munchies-modern": "0.3.0",
    "@stefanmaric/eslint-config-munchies-modules": "0.3.0",
    "@stefanmaric/eslint-config-munchies-node": "0.3.0",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vercel/ncc": "0.30.0",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "jest": "27.1.0",
    "jest-circus": "27.1.0",
    "js-yaml": "4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "27.0.5",
    "typescript": "^4.4.2"
  }
}
